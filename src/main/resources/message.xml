<?xml version="1.0" encoding="UTF-8" ?>
<protocols version="1.0">

    <!--序列号0用来占位,不要使用0作为id !!!-->

    <!--公共服务-->
    <message id="-1" value="game.common.packet.SM_Message"/>

    <!--用户登录 1 - 10 -->
    <message id="1" value="game.user.login.packet.CM_UserLogin"/>
    <message id="2" value="game.user.login.packet.CM_UserLogout"/>
    <message id="3" value="game.user.login.packet.SM_LoginSuccess"/>
    <message id="4" value="game.user.login.packet.CM_UserRegister"/>

    <!--gm命令 51 - 70-->
    <message id="51" value="game.gm.packet.CM_GmCommand"/>

    <!--玩家背包相关 91 - 120 -->
    <message id="91" value="game.user.pack.packet.SM_PackInfo"/>
    <message id="92" value="game.user.pack.packet.CM_PackInfo"/>

    <!--玩家装备栏相关  140 - 160 -->
    <message id="140" value="game.user.equip.packet.CM_EquipStorage"/>
    <message id="141" value="game.user.equip.packet.SM_EquipStorage"/>

    <!--地图协议根据id走地图线程池-->
    <!--地图基础协议 2001 - 2005-->
    <message id="2001" value="game.scene.map.packet.CM_EnterMap"/>
    <message id="2002" value="game.scene.map.packet.CM_LeaveMap"/>
    <message id="2003" value="game.scene.map.packet.CM_MoveMap"/>
    <message id="2004" value="game.scene.map.packet.CM_TransferMap"/>
    <message id="2005" value="game.scene.map.packet.CM_ChangeMap"/>

</protocols>
